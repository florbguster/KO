@{
    ViewBag.Title = "Second";
}
<div id="" class="contentDiv">
    <h4>Observing...</h4>

    <input type="text" 
            placeholder="Type something" 
            data-bind="value: typedWord, 
                        valueUpdate: 'afterkeydown'"/>

    <div id="AppendedResultSection">
        @for (int i = 0; i < 100; i++)
        {
            <span data-bind="text: typedWord"></span>            
        }
    </div>

 </div>
 <script src="@Url.Content("~/Scripts/ContentScripts/Observables.js")" type="text/javascript"></script>